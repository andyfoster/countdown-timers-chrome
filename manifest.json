{
  "name": "Countdown Timers",
  "version": "0.0.1",
  "description": "A simple Chrome extension that lets users create and manage countdown timers on their new tab page.",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "192": "icons/icon192.png",
    "512": "icons/icon512.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
